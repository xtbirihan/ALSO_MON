       <REFERENCE>X</REFERENCE>
       <TYP>/SCWM/LGNUM</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IV_VARIANT</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>VARIANT</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IV_MODE</PARAMETER>
       <DEFAULT>&apos;1&apos;</DEFAULT>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>/SCWM/DE_MON_FM_MODE</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IT_DATA_PARENT</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>/SCWM/TT_WIP_WHRHEAD_OUT</TYP>
